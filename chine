import java.util.Scanner;
public class chineZodiaq {

    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.print("please entry born year: ");
        int bornYear = input.nextInt();
        int zodiaq = (bornYear%12);
        
        switch (zodiaq) {
            case 0:
                System.out.println("your zodiaq horowere is monkey");
                
                break;
            case 1: 
                System.out.println("your zodiaq horowere is turkey");
                break;
            case 2 : 
                System.out.println("your zodiaq horowere is dog");
                break;
            case 3 : 
                System.out.println("your zodiaq horowere is pig");
                break;
            case 4 : 
            System.out.println("your zodiaq horowere is mouse");
            break;
            case 5:
            System.out.println("your zodiaq horowere is ox");
            break;
            case 6 : 
            System.out.println("your zodiaq horowere is tiger");
            break;
            case 7: 
            System.out.println("your zodiaq horowere is rabbit");
            break;
            case 8 : 
            System.out.println("your zodiaq horowere is dragon");
            break;
            case 9: 
            System.out.println("your zodiaq horowere is snake");
            break;
            case 10:
            System.out.println("your zodiaq horowere is horse");
            break;
            case 11:
            System.out.println("your zodiaq horowere is sheep");
            break;
          
            }input.close();
    }
}
